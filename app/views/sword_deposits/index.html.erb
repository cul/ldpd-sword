<p id="notice"><%= notice %></p>

<h1>Listing Sword Deposits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sword_deposits.each do |sword_deposit| %>
      <tr>
        <td><%= link_to 'Show', sword_deposit %></td>
        <td><%= link_to 'Edit', edit_sword_deposit_path(sword_deposit) %></td>
        <td><%= link_to 'Destroy', sword_deposit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sword deposit', new_sword_deposit_path %>
