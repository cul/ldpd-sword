<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @collection.name %>
</p>

<p>
  <strong>Slug:</strong>
  <%= @collection.slug %>
</p>

<p>
  <strong>Hyacinth Project (string key):</strong>
  <%= @collection.hyacinth_project_string_key %>
</p>

<p>
  <strong>Parser:</strong>
  <%= @collection.parser %>
</p>

<% if current_user.admin? %>
  <%= link_to 'Edit', edit_collection_path(@collection) %> |
  <%= link_to 'Delete', @collection, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>
